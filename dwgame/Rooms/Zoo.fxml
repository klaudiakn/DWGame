            <Font name="Cinzel-Regular" size="14.0" />
         </font>
      </Label>
      <Button fx:id="chop" layoutX="52.0" layoutY="352.0" mnemonicParsing="false" onAction="#karateChopIt" text="Karate Chop It" />
      <Button fx:id="ball" layoutX="200.0" layoutY="352.0" mnemonicParsing="false" onAction="#throwBall" text="Throw a ball" />
      <Button fx:id="fire" layoutX="350.0" layoutY="352.0" mnemonicParsing="false" onAction="#fire" text="Fire with weapon" />
      <Button fx:id="door" layoutX="187.0" layoutY="406.0" mnemonicParsing="false" onAction="#door" prefHeight="25.0" prefWidth="107.0" text="Go to the door" visible="false" />
