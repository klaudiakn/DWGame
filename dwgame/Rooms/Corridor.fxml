<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" minHeight="122.0" prefHeight="467.0" prefWidth="455.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dwgame.Rooms.CorridorController">
  <children>
      <VBox alignment="CENTER" prefHeight="467.0" prefWidth="508.0">
         <children>
        <Label prefHeight="214.0" prefWidth="321.0" text="You're standing in the corridor. As you go further, you see doors to three rooms. One looks like a giant library the second one is a swimming pool and then there is wardrobe. Which one do you want to enter?" wrapText="true">
          <font>
            <Font name="CinzelDecorative-Black" size="12.0" />
          </font>
        </Label>
            <HBox alignment="BOTTOM_CENTER" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
               <children>
                  <Button fx:id="library" mnemonicParsing="false" onAction="#goToLibrary" prefHeight="25.0" prefWidth="101.0" text="Library" />
              <Button fx:id="wardrobe" mnemonicParsing="false" onAction="#goToWardrobe" prefHeight="25.0" prefWidth="102.0" text="Wardrobe" />
              <Button fx:id="swimmingPool" mnemonicParsing="false" onAction="#goToSwimmingPool" text="Swimming Pool" />
               </children>
            </HBox>
         </children>
      </VBox>
  </children>
</AnchorPane>
